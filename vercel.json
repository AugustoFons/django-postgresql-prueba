{
    "version": 2,
    "builds": [
        {
            "src": "Entrega3/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "static/assets",
            "use": "@vercel/static",
            "config": {}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/assets/(.*)",
            "dest": "/static/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Entrega3/wsgi.py"
        }
    ]
}